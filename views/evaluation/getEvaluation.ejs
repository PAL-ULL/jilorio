<!DOCTYPE html>
<html lang="en">

<head>
    <title>Evaluaciones registradas en la aplicaci√≥n - Jilorio</title>
    <link rel="shortcut icon" href="https://doorcountypulse.com/wp-content/uploads/2020/03/forkspoon.png">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta charset="UTF-8">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css"
        integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.1.3/css/bootstrap.css">
    <link rel="stylesheet" href="https://cdn.datatables.net/1.10.21/css/dataTables.bootstrap4.min.css">
    <link rel="stylesheet" type="text/css" href="/stylesheets/food.css" />
    <link rel="stylesheet" type="text/css" href="/stylesheets/style.css" />
    <link rel="stylesheet" type="text/css" href="/stylesheets/dish.css"/>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.13.0/css/all.css">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.13.0/css/v4-shims.css">
    <script src="https://code.jquery.com/jquery-3.5.1.min.js"
    integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
    <link rel="stylesheet" type="text/css" href="/stylesheets/fondo.css" />
    <!-- JQUERY AUTOCOMPLETE -->
    <link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
    <link rel="stylesheet" href="/resources/demos/style.css">
    <script src="https://code.jquery.com/jquery-1.12.4.js"></script>
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>




    

    


</head>

<body>
    <%- include ("../partials/navigation.ejs"); %>
    <div class="container">
    
        <div class="infoPage">
            <i class="fas fa-list-alt fa-2x"></i>
            <span> <%= items.myObject.infoPage.evaluaciones %> </span>
        </div>
        <hr style="border-color: rgb(46, 115, 124);">
        <%- messages('message', locals) %>
        <br>
        

        <form method="POST">
            <div class="wrap">
                <div class="search">
                    <input type="text" name="_id" class="searchTerm" placeholder="<%= items.myObject.busqueda %>">
                    <a id="submit-form-link" href="#"></a>
                    <button type="submit" class="spanColor searchButton">
                        <i class="fa fa-search"></i>
                    </button>
                </div>
            </div>
        </form>
        <br><br>


        <% if ((items.myObject.tablaEvaluacion.valores.length) > 0) { %>
            <div class="cardInside3">
            <table id="evaluations" class="table table-bordered table-hover table-sm">
                <thead class="thead-dark">
                    <tr>

                        <% for (var i=0; i< items.myObject.tablaEvaluacion.nombres.length; i++) { %>
                        <th> <%- items.myObject.tablaEvaluacion.nombres[i] %> </th>
                        <% } %>

                    </tr>
                </thead>
                <tbody>
                    <% for (var i=0; i< items.myDocs.length; i++) { %>

                        <tr>
                            <td> <%= items.myDocs[i].candidato %> </td>
                            <td> <%= items.myDocs[i].recomendacion %> </td>

                            <% if (items.myDocs[i].resultados[0]["energia"][1] === "No recomendable") { %>
                                <td class="text-danger"> <%= items.myDocs[i].resultados[0]["energia"][0] %> <br><%= items.myDocs[i].resultados[0]["energia"][1] %>  </td>
                            <% } else if (items.myDocs[i].resultados[0]["energia"][1] === "Recomendable") {   %>
                                <td class="text-warning"> <%= items.myDocs[i].resultados[0]["energia"][0] %> <br><%= items.myDocs[i].resultados[0]["energia"][1] %>  </td>
                            <% } else{ %>
                                <td class="text-success"> <%= items.myDocs[i].resultados[0]["energia"][0] %> <br><%= items.myDocs[i].resultados[0]["energia"][1] %>  </td>
                                <% } %>

                            <% if (items.myDocs[i].resultados[0]["lipidos"][1] === "No recomendable") { %>
                                <td class="text-danger"> <%= items.myDocs[i].resultados[0]["lipidos"][0] %> <br><%= items.myDocs[i].resultados[0]["lipidos"][1] %> </td>
                            <% } else if (items.myDocs[i].resultados[0]["lipidos"][1] === "Recomendable") {   %>
                                <td class="text-warning"> <%= items.myDocs[i].resultados[0]["lipidos"][0] %> <br><%= items.myDocs[i].resultados[0]["lipidos"][1] %> </td>
                            <% } else{ %>
                                <td class="text-success"> <%= items.myDocs[i].resultados[0]["lipidos"][0] %> <br><%= items.myDocs[i].resultados[0]["lipidos"][1] %> </td>
                                <% } %>

                            <% if (items.myDocs[i].resultados[0]["proteinas"][1] === "No recomendable") { %>
                                <td class="text-danger"> <%= items.myDocs[i].resultados[0]["proteinas"][0] %> <br><%= items.myDocs[i].resultados[0]["proteinas"][1] %>  </td>
                            <% } else if (items.myDocs[i].resultados[0]["proteinas"][1] === "Recomendable") {   %>
                                <td class="text-warning"> <%= items.myDocs[i].resultados[0]["proteinas"][0] %> <br><%= items.myDocs[i].resultados[0]["proteinas"][1] %>  </td>
                            <% } else{ %>
                                <td class="text-success"> <%= items.myDocs[i].resultados[0]["proteinas"][0] %> <br><%= items.myDocs[i].resultados[0]["proteinas"][1] %>  </td>
                                <% } %>

                            <% if (items.myDocs[i].resultados[0]["carbohidratos"][1] === "No recomendable") { %>
                                <td class="text-danger"> <%= items.myDocs[i].resultados[0]["carbohidratos"][0] %> <br><%= items.myDocs[i].resultados[0]["carbohidratos"][1] %>  </td>
                            <% } else if (items.myDocs[i].resultados[0]["carbohidratos"][1] === "Recomendable") {   %>
                                <td class="text-warning"> <%= items.myDocs[i].resultados[0]["carbohidratos"][0] %> <br><%= items.myDocs[i].resultados[0]["carbohidratos"][1] %>  </td>
                            <% } else{ %>
                                <td class="text-success"> <%= items.myDocs[i].resultados[0]["carbohidratos"][0] %> <br><%= items.myDocs[i].resultados[0]["carbohidratos"][1] %>  </td>
                                <% } %>
                
                            <td> <button type="button" class="btn btn-danger"><a title="Eliminar" href="/evaluation/delete/<%=items.myDocs[i]['_id']%>"> <i class="fas fa-trash-alt"></i></a></button>
                               
                            </td>
                        </tr>
                        <% } %>
                   
                </tbody>
     

            </table>
            <% } %>
        </div>
            <br>
    </div>
    <%- include ("../partials/footer.ejs"); %>

    <script>
    $(document).ready(function () {
        $('#evaluations').DataTable({
            "language": {

                "url": "//cdn.datatables.net/plug-ins/9dcbecd42ad/i18n/Spanish.json",

                "emptyTable": " "
            }
        });
    });
</script>

<script src="https://cdn.datatables.net/1.10.21/js/jquery.dataTables.min.js"></script>
<script src="https://cdn.datatables.net/1.10.21/js/dataTables.bootstrap4.min.js"></script>
</body>


</html>